        <domain includeSubdomains="false">192.168.1.2</domain>
        <domain includeSubdomains="false">192.168.1.100</domain>
        <domain includeSubdomains="false">192.168.1.101</domain>
        <domain includeSubdomains="false">192.168.1.102</domain>
        <domain includeSubdomains="false">192.168.1.103</domain>
        <domain includeSubdomains="false">192.168.1.104</domain>
        <domain includeSubdomains="false">192.168.1.105</domain>
        
        <domain includeSubdomains="false">192.168.2.1</domain>
        <domain includeSubdomains="false">192.168.2.2</domain>
        <domain includeSubdomains="false">192.168.2.100</domain>
        <domain includeSubdomains="false">192.168.2.101</domain>
        <domain includeSubdomains="false">192.168.2.102</domain>
        <domain includeSubdomains="false">192.168.2.103</domain>
        <domain includeSubdomains="false">192.168.2.104</domain>
        <domain includeSubdomains="false">192.168.2.105</domain>
        <domain includeSubdomains="false">192.168.2.146</domain>
        
        <domain includeSubdomains="false">192.168.0.1</domain>
