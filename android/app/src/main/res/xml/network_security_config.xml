        <domain includeSubdomains="false">10.0.0.2</domain>
        <domain includeSubdomains="false">10.0.0.100</domain>
    </domain-config>

    <domain-config cleartextTrafficPermitted="true">
        <domain includeSubdomains="true">cdn.sono.wtf</domain>
    </domain-config>
    
